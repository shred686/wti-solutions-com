---

<Layout title="Contract Vehicles - WTI Solutions">
  <Header transparent={false} />
  
  <!-- Page Header -->
  <PageHeader title="Contract Vehicles" />
  
  <!-- Contract Vehicles Section -->
  <section class="py-16 bg-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <!-- Introduction -->
      <div class="text-center mb-12">
        <h2 class="text-3xl md:text-4xl font-light text-slate-800 mb-6">
          Our Contract Vehicles
        </h2>
        <p class="text-lg text-slate-600 max-w-4xl mx-auto leading-relaxed">
          WTI holds prime positions on multiple government contract vehicles, providing federal agencies with streamlined access to our technology solutions and professional services.
        </p>
      </div>
      
            <!-- Tab System Container -->
      <div class="flex flex-col lg:flex-row">
        <!-- Vertical Tab Navigation (skinny left side) -->
        <div class="w-48 flex-shrink-0 bg-white border border-gray-200 rounded-lg overflow-hidden shadow-sm">
          {contractVehicles.map((contract, index) => (
            <button 
              class={`contract-tab w-full h-16 text-left px-4 font-medium transition-all duration-300 border-b border-gray-100 last:border-b-0 text-lg flex items-center justify-center ${
                index === 0 
                  ? 'bg-gray-800 text-white' 
                  : 'bg-white text-gray-700 hover:bg-gray-50 hover:text-[#556b2f]'
              }`}
              data-contract={contract.id}
            >
              <span class="font-semibold">{contract.name}</span>
            </button>
          ))}
        </div>
        
        <!-- Content Area (takes up remaining space) -->
        <div class="lg:flex-1 bg-white border border-gray-200 rounded-lg p-8 shadow-sm">
            {contractVehicles.map((contract, index) => (
              <div 
                id={`${contract.id}-content`}
                class={`contract-content ${index === 0 ? 'block' : 'hidden'}`}
              >
                                                 <div class="space-y-6">
                  <div class="flex items-start justify-between">
                    <div>
                      {contract.hasLogo ? (
                        <div class="mb-2">
                          <img src="/assets/icons/oasis-logo.jpg" alt="OASIS+ Logo" class="h-24 w-auto" />
                        </div>
                      ) : (
                        <h2 class="text-3xl font-light mb-2">{contract.name}</h2>
                      )}
                      <h3 class="text-xl text-gray-600 mt-2">{contract.fullName}</h3>
                    </div>
                  </div>
                  
                  <p class="text-lg leading-relaxed">{contract.description}</p>
                   
                   <div class="bg-gray-50 rounded-lg p-6 border border-gray-100">
                     <p class="leading-relaxed">{contract.details}</p>
                     
                     {contract.capabilities && (
                       <div class="mt-6 space-y-4">
                         {contract.capabilities.map(capability => (
                           <p class="leading-relaxed" set:html={capability}></p>
                         ))}
                       </div>
                     )}
                     
                     {contract.taskAreas && (
                       <div class="mt-6">
                         <h4 class="text-lg font-medium mb-3">Task orders will focus on the following task areas:</h4>
                         <ul class="space-y-2">
                           {contract.taskAreas.map(taskArea => (
                             <li class="flex items-center">
                               <svg class="w-4 h-4 mr-3 text-[#556b2f] flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                                 <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"></path>
                               </svg>
                               {taskArea}
                             </li>
                           ))}
                         </ul>
                       </div>
                     )}
                     
                     {contract.marketSegments && (
                       <div class="mt-6">
                         <div class="space-y-2">
                           {contract.marketSegments.map((segment, index) => (
                             <div class="flex items-start">
                               <span class="text-[#556b2f] font-medium mr-3 mt-0.5 w-6 text-center">{index + 1}.</span>
                               <span class="text-gray-700">{segment}</span>
                             </div>
                           ))}
                         </div>
                       </div>
                     )}
                     
                     {contract.conclusion && (
                       <p class="leading-relaxed mt-6">{contract.conclusion}</p>
                     )}
                     
                     {contract.additionalInfo && (
                       <div class="mt-6 space-y-4">
                         {contract.additionalInfo.map(info => (
                           <p class="leading-relaxed">{info}</p>
                         ))}
                       </div>
                     )}
                     
                     {contract.sin && (
                       <div class="mt-4">
                         <p class="font-semibold text-[#556b2f]">{contract.sin}</p>
                       </div>
                     )}
                     
                     {contract.contractDetails && (
                       <div class="mt-6">
                         <h4 class="text-lg font-medium mb-3">Contract Details:</h4>
                         <div class="ml-4 space-y-1">
                           {contract.contractDetails.contractNumber && (
                             <p><strong>Contract Number:</strong> {contract.contractDetails.contractNumber}</p>
                           )}
                           {contract.contractDetails.period && (
                             <p><strong>Period of Performance:</strong> {contract.contractDetails.period}</p>
                           )}
                           {contract.contractDetails.ceiling && (
                             <p><strong>Ceiling:</strong> {contract.contractDetails.ceiling}</p>
                           )}
                         </div>
                       </div>
                     )}
                     
                     {contract.contact && contract.contact.title && (
                       <div class="mt-6">
                         <h4 class="text-lg font-medium mb-3">{contract.contact.title}</h4>
                         <div class="ml-4 space-y-1">
                           {contract.contact.email && <p><strong>Email:</strong> {contract.contact.email}</p>}
                           {contract.contact.phone && <p><strong>Phone:</strong> {contract.contact.phone}</p>}
                         </div>
                       </div>
                     )}
                     
                     {contract.contact && contract.contact.email && !contract.contact.title && (
                       <div class="mt-6">
                         <p><strong>For more information, contact:</strong> {contract.contact.email}</p>
                       </div>
                     )}
                     
                     {contract.awardedPools && (
                       <div class="mt-6">
                         <h4 class="text-lg font-medium mb-3">Our awarded pools are:</h4>
                         <ul class="space-y-2">
                           {contract.awardedPools.map(pool => (
                             <li class="flex items-center">
                               <svg class="w-4 h-4 mr-3 text-[#556b2f] flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                                 <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"></path>
                               </svg>
                               {pool}
                             </li>
                           ))}
                         </ul>
                       </div>
                     )}
                     
                     {contract.links && contract.links.length > 0 && (
                       <div class="mt-6">
                         <ul class="ml-4 space-y-1">
                           {contract.links.map(link => (
                             <li><a href={link.url} class="text-[#556b2f] hover:underline">{link.text}</a></li>
                           ))}
                         </ul>
                       </div>
                     )}
                     
                     {contract.hasCapabilitiesStatement && (
                       <div class="mt-6">
                         <a href={`${baseUrl}WTI-Capabilities-Statement.pdf`} target="_blank" rel="noopener noreferrer" class="inline-flex items-center px-6 py-3 border border-transparent text-base font-medium rounded-lg text-white bg-slate-800 hover:bg-slate-700 transition-colors duration-300">
                           Capabilities Statement
                         </a>
                       </div>
                     )}
                     
                     {contract.domains && (
                       <div class="mt-4">
                         <h4 class="text-lg font-medium mb-3">Contract Domains:</h4>
                         <ul class="space-y-2">
                           {contract.domains.map((domain: string) => (
                             <li class="flex items-center">
                               <svg class="w-4 h-4 mr-3 text-[#556b2f] flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                                 <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"></path>
                               </svg>
                               {domain}
                             </li>
                           ))}
                         </ul>
                       </div>
                     )}
                   </div>
                 </div>
              </div>
            ))}
          </div>
        </div>
      </div>
    </div>
  </section>
  
  <!-- Contact Section -->
  <section class="py-16 bg-gray-50">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="text-center">
        <h2 class="text-3xl md:text-4xl font-light text-gray-900 mb-6">
          Ready to Partner?
        </h2>
        <p class="text-lg text-gray-600 mb-8 max-w-2xl mx-auto">
          Contact us to learn more about how we can support your mission through these contract vehicles.
        </p>
        
        <div class="flex flex-col sm:flex-row gap-4 justify-center">
          <a href={`${baseUrl}contact`} class="inline-flex items-center px-8 py-3 border border-transparent text-base font-medium rounded-lg text-white bg-slate-800 hover:bg-slate-700 transition-colors duration-300">
            Contact Us
          </a>
        </div>
      </div>
    </div>
  </section>
  
  <Footer />
</Layout>

<script>
  // Contract vehicles tab functionality
  const contractTabs = document.querySelectorAll('.contract-tab');
  const contractContents = document.querySelectorAll('.contract-content');
  
  contractTabs.forEach(tab => {
    tab.addEventListener('click', () => {
      const contractId = tab.getAttribute('data-contract');
      const targetContent = document.getElementById(`${contractId}-content`);
      
             // Update tab styles
       contractTabs.forEach(t => {
         t.classList.remove('bg-gray-800', 'text-white');
         t.classList.add('bg-white', 'text-gray-700');
       });
       tab.classList.remove('bg-white', 'text-gray-700');
       tab.classList.add('bg-gray-800', 'text-white');
      
      // Update content visibility
      contractContents.forEach(content => {
        content.classList.add('hidden');
        content.classList.remove('block');
      });
      if (targetContent) {
        targetContent.classList.remove('hidden');
        targetContent.classList.add('block');
      }
    });
  });
</script>

<style>
  .contract-tab {
    transition: all 0.3s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  }
  
  .contract-content {
    animation: fadeIn 0.3s ease-out;
  }
  
  @keyframes fadeIn {
    from {
      opacity: 0;
      transform: translateY(10px);
    }
    to {
      opacity: 1;
      transform: translateY(0);
    }
  }
</style> 